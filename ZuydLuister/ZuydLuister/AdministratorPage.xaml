<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ZuydLuister.AdministratorPage"
             xmlns:local="clr-namespace:ZuydLuister"
            x:Name="administratorPage">

    <local:ScenariosOverviewPage Title="Scenario's"/>
    <local:CategoryOverviewPage Title="Categorieën"/>
    <local:AdministratorOverviewPage x:Name="administratorTab" Title="Beheerders"/>

    <TabbedPage.ToolbarItems>
        <ToolbarItem x:Name="menuToolbarItem" Text="Menu" Clicked="menuToolbarItem_Clicked"/>
    </TabbedPage.ToolbarItems>
</TabbedPage>

